@{
    Layout = null;
    var localVar = ViewData["hdnFlag"];
}
<!DOCTYPE html>
<html lang="en">

<head>
    <title>VanSale</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--===============================================================================================-->
    <link rel="icon" type="image/png" href="~/Sang_solutions/light/logasset/images/favicon.png" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Sang_solutions/light/logasset/vendor/bootstrap/css/bootstrap.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Sang_solutions/light/logasset/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Sang_solutions/light/logasset/fonts/Linearicons-Free-v1.0.0/icon-font.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Sang_solutions/light/logasset/vendor/animate/animate.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Sang_solutions/light/logasset/vendor/css-hamburgers/hamburgers.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Sang_solutions/light/logasset/vendor/animsition/css/animsition.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Sang_solutions/light/logasset/vendor/select2/select2.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Sang_solutions/light/logasset/vendor/daterangepicker/daterangepicker.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Sang_solutions/light/logasset/css/util.css">
    <link rel="stylesheet" type="text/css" href="~/Sang_solutions/light/logasset/css/main.css">
    <!--===============================================================================================-->
</head>

<body style="background-color: #666666;">

    <div class="limiter">
        <div class="container-login100">
            <div class="wrap-login100">
                <form class="login100-form validate-form">
                    @* <input type="text" asp-for="@localVar" />*@
                    <input type="hidden" value="@ViewData["hdnFlag"]" />
                    <input type="hidden" id="hdn_SessionUID" class="form-control" name="hdn_SessionUID" value="@ViewBag.UID" />

                    <span class="login100-form-title p-b-43">
                        <h1 class="my_txt">Login</h1>
                    </span>


                    <div class="wrap-input100 validate-input">
                        @*data-validate="Valid email is required: ex@abc.xyz"*@
                        <input class="input100" type="text" name="email" id="sLogin">
                        <span class="focus-input100"></span>
                        <span class="label-input100">Email</span>
                    </div>


                    <div class="wrap-input100 validate-input">
                        @*data-validate="Password is required"*@
                        <input class="input100" type="password" name="pass" id="sPass">
                        <span class="focus-input100"></span>
                        <span class="label-input100">Password</span>
                    </div>

                    <div class="flex-sb-m w-full p-t-3 p-b-32">
                        @*<div class="contact100-form-checkbox">
                                <input class="input-checkbox100" id="RememberMe" type="checkbox" name="remember-me">
                                <label class="label-checkbox100" for="ckb1">
                                    Remember me
                                </label>
                            </div>*@

                        <div class="contact100-form-checkbox" style="padding-left:30px">
                            <input type="checkbox" class="form-check-input" id="RememberMe">
                            <label for="ckb1">
                                Remember me
                            </label>
                        </div>

                        <div>
                            <a href="#" class="txt1">
                                Forgot Password?
                            </a>
                        </div>
                    </div>
                    <div class="container-login100-form-btn">
                        <button id="btnLogin" class="login100-form-btn">Login</button>

                    </div> <div class="my-3">
                        <label id="ErrorInfo" style="color: orangered;"></label>
                        <label id="SuccessInfo" style="color: green;"></label>
                        <div>
                        </div>

                    </div>
                    <div class="clearfix"></div>
                    <div class="mt-3">
                        <div class="login100-form-social flex-c-m">
                            <a href="#" class="login100-form-social-item flex-c-m bg1 m-r-5">
                                <i class="fa fa-facebook-f" aria-hidden="true"></i>
                            </a>

                            <a href="#" class="login100-form-social-item flex-c-m bg2 m-r-5">
                                <i class="fa fa-twitter" aria-hidden="true"></i>
                            </a>
                        </div>
                    </div>
                </form>

                <div class="login100-more">
                    <img class="my_img" src="~/Sang_solutions/light/logasset/images/login_bg.jpg" alt="">
                </div>
            </div>
        </div>
    </div>





    <!--===============================================================================================-->
    <script src="~/Sang_solutions/light/logasset/vendor/jquery/jquery-3.2.1.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/Sang_solutions/light/logasset/vendor/animsition/js/animsition.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/Sang_solutions/light/logasset/vendor/bootstrap/js/popper.js"></script>
    <script src="~/Sang_solutions/light/logasset/vendor/bootstrap/js/bootstrap.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/Sang_solutions/light/logasset/vendor/select2/select2.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/Sang_solutions/light/logasset/vendor/daterangepicker/moment.min.js"></script>
    <script src="~/Sang_solutions/light/logasset/vendor/daterangepicker/daterangepicker.js"></script>
    <!--===============================================================================================-->
    <script src="~/Sang_solutions/light/logasset/vendor/countdowntime/countdowntime.js"></script>
    <!--===============================================================================================-->
    <script src="~/Sang_solutions/light/logasset/js/main.js"></script>
    <script src="~/js/Scripts/CustomScripts/Login.js" asp-append-version="true"></script>
    <script src="~/js/Scripts/jquery.cookie.min.js"></script>
    @*<script src="~/Scripts/CustomScripts/Login.js"></script>*@

</body>

</html>